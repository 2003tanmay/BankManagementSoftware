<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>LoginChecker</servlet-name>
        <servlet-class>controller.LoginChecker</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BalanceChecker</servlet-name>
        <servlet-class>controller.BalanceChecker</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterChecker</servlet-name>
        <servlet-class>controller.RegisterChecker</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MoneyDepositor</servlet-name>
        <servlet-class>controller.MoneyDepositor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MoneyDrawer</servlet-name>
        <servlet-class>controller.MoneyDrawer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BalanceChecker</servlet-name>
        <url-pattern>/BalanceChecker</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterChecker</servlet-name>
        <url-pattern>/RegisterChecker</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MoneyDepositor</servlet-name>
        <url-pattern>/MoneyDepositor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MoneyDrawer</servlet-name>
        <url-pattern>/MoneyDrawer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginChecker</servlet-name>
        <url-pattern>/LoginChecker</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            59
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
